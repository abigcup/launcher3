<resources>
	<style name="pop_update_content" parent="whauto">
		<item name="android:layout_width">@dimen/dp256</item>
		<item name="android:orientation">vertical</item>
		<item name="android:layout_centerInParent">true</item>
	</style>
	<style name="pop_update_top_bg" parent="hauto">
		<item name="android:src">@drawable/bg_migu_update_top</item>
	</style>
	<style name="pop_update_bottom" parent="hauto">
		<item name="android:background">@drawable/bg_pop_desktop_bottom</item>
		<item name="android:orientation">vertical</item>
		<item name="android:paddingLeft">@dimen/dp24</item>
		<item name="android:paddingRight">@dimen/dp24</item>
		<item name="android:paddingBottom">@dimen/dp20</item>
	</style>
	<style name="pop_update_notice" parent="hauto">
		<item name="android:textSize">@dimen/fs14</item>
		<item name="android:textColor">#333</item>
		<item name="android:gravity">center</item>
		<item name="android:layout_marginTop">@dimen/dp4</item>
		<item name="android:layout_marginBottom">@dimen/dp20</item>
	</style>
	<style name="pop_update_btn" parent="hauto">
		<item name="android:orientation">horizontal</item>
	</style>
	<style name="pop_update_btn_soild">
		<item name="android:layout_height">@dimen/dp34</item>
		<item name="android:gravity">center</item>
		<item name="android:layout_width">match_parent</item>
		<item name="android:layout_weight">1</item>
		<item name="android:layout_marginLeft">@dimen/dp10</item>
		<item name="android:layout_marginRight">@dimen/dp10</item>
		<item name="android:textColor">@color/blue_0099ff</item>
		<item name="android:textSize">@dimen/fs14</item>
		<item name="android:background">@drawable/bg_rec4_blue_soild</item>
	</style>
	<style name="pop_update_btn_blue" parent="pop_update_btn_soild">
		<item name="android:textColor">@color/white</item>
		<item name="android:background">@drawable/bg_rec4_blue</item>
	</style>
	<style name="pop_update_progress" parent="hauto">
		<item name="android:orientation">vertical</item>
	</style>
	<style name="pop_update_progress_body" parent="Base.Widget.AppCompat.ProgressBar.Horizontal">
		<item name="android:layout_width">match_parent</item>
		<item name="android:layout_height">@dimen/dp14</item>
		<item name="android:progressDrawable">@drawable/bg_pop_update_progress</item>
	</style>
	<style name="pop_update_progress_notice" parent="hauto">
		<item name="android:layout_marginTop">@dimen/dp10</item>
		<item name="android:textSize">@dimen/fs12</item>
		<item name="android:textColor">#333</item>
		<item name="android:gravity">center</item>
	</style>
</resources>